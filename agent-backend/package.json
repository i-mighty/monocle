{
  "name": "agent-backend",
  "version": "0.0.1",
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/app.ts",
    "start": "node dist/app.js",
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "openapi:validate": "npx @redocly/cli lint openapi.yaml",
    "openapi:generate-types": "npx openapi-typescript openapi.yaml -o src/types/generated.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^4.19.2",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.2",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
